# Environment Variables Example
# Copy this to .env and fill in your actual values
# Add .env to .gitignore to keep credentials secure

# Redis Configuration
REDIS_HOST=redis-18141.c85.us-east-1-2.ec2.cloud.redislabs.com
REDIS_PORT=18141
REDIS_PASSWORD=your-redis-password-here

# JWT Configuration (generate with: openssl rand -base64 32)
JWT_SECRET=your-secure-jwt-secret-key-here-minimum-256-bits
JWT_EXPIRATION=86400000

# Database Configuration
DATABASE_URL=jdbc:sqlite:test.db
